{
  "name": "minecraft-wiki-toolkit",
  "version": "0.1.0",
  "description": "Un wiki Minecraft avancé avec outils d'analyse et générateur de blueprints",
  "main": "server/src/index.js",
  "scripts": {
    "start": "node server/src/index.js",
    "server:dev": "nodemon server/src/index.js",
    "client:dev": "cd client && npm start",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "install:all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kihw/minecraft-wiki-toolkit.git"
  },
  "keywords": [
    "minecraft",
    "wiki",
    "blueprints",
    "calculators",
    "gaming"
  ],
  "author": "kihw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kihw/minecraft-wiki-toolkit/issues"
  },
  "homepage": "https://github.com/kihw/minecraft-wiki-toolkit#readme",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.6.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "prismarine-nbt": "^2.2.1",
    "three": "^0.157.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3"
  }
}